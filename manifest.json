{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.instructure.com/courses/*/quizzes/*/take"],
      "js": ["contentScript.js"]
    }
  ]
}